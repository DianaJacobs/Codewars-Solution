# https://www.codewars.com/kata/57a429e253ba3381850000fb

def bmi(w, h):
    h = h * h
    b = w / h
    if b <= 18.5:
        return "Underweight"
    elif b <= 25:
        return "Normal"
    elif b <= 30:
        return "Overweight"
    elif b > 30:
        return "Obese"
    else: return "This is not an option"
